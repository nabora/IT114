#include <stdio.h>
int main()
{
   float marks1, marks2, marks3, marks4, marks5, average;
   
   printf("Enter marks in subject 1:\n");
   scanf("%f", &marks1);
   printf("Enter marks in subject 2:\n");
   scanf("%f", &marks2);
   printf("Enter marks in subject 3:\n");
   scanf("%f", &marks3);
   printf("Enter marks in subject 4:\n");
   scanf("%f", &marks4);
   printf("Enter marks in subject 5:\n");
   scanf ("%f", &marks5);
   
   average = (marks1 + marks2 + marks3 + marks4 + marks5)/5;
   printf("Average: %f\n", average);
  
  
  return 0;
}
